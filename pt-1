    largeArr = arr1;
    smallArr = arr2
  }
  else {
    largeArr = arr2;
    smallArr = arr1;
  }
  for (let i = 0; i < largeArr.length; i++) {
    
  }
  return newArr;
}

diffArray([1, 2, 3, 5], [1, 2, 3, 4, 5]);
